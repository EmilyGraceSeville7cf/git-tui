{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "color": {
      "type": "string",
      "oneOf": [
        {
          "examples": [
            "black",
            "red",
            "green",
            "yellow",
            "blue",
            "magenta",
            "cyan",
            "white",
            "bold",
            "default",
            "reverse",
            "underline",
            "strikethrough"
          ]
        },
        {
          "pattern": "^#[0-9a-fA-F]{6}$",
          "examples": [
            "#ff00ff"
          ]
        }
      ]
    }
  },
